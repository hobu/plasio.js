{
	"source": {
		"include": ["lib"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"node_modules/jsdoc-strip-async-await",
        "node_modules/jsdoc-babel",
		"plugins/markdown"
	],
	"opts": {
		"template": "node_modules/docdash",
		"destination": "./docs/"
		
	}
}
